{
  "name": "api-template",
  "version": "0.1.0",
  "description": "API typescript template.",
  "main": "lambda.js",
  "config": {
    "s3BucketName": "deploy",
    "region": "us-east-1",
    "cloudFormationStackName": "",
    "functionName": "",
    "domain": "''"
  },
  "scripts": {
    "ambiente": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./debug/SetarAmbienteLocal.ps1",
    "dev": "ts-node-dev --inspect --respawn --transpile-only src/server.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "postbuild": "shx cp package*json dist/ && shx cp .env* dist/",
    "prod": "node dist/server.js",
    "sam:build": "sam build",
    "sam:package": "sam package --s3-bucket $npm_package_config_s3BucketName --s3-prefix $npm_package_name --region $npm_package_config_region",
    "sam:deploy": "cross-var sam deploy --stack-name $npm_package_config_cloudFormationStackName --capabilities CAPABILITY_IAM --s3-bucket $npm_package_config_s3BucketName --s3-prefix $npm_package_name --parameter-overrides DomainName=$npm_package_config_domain --region $npm_package_config_region --no-fail-on-empty-changeset",
    "package-deploy": "npm run build && npm run sam:build && npm run sam:deploy",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/passport": "^1.0.7",
    "@types/passport-http-bearer": "^1.0.37",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "cross-var": "^1.1.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.4.1",
    "shx": "^0.3.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@vendia/serverless-express": "^4.5.3",
    "axios": "^0.26.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1"
  }
}
